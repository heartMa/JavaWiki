<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.w3cjava.modules.test.mapper.TestDao">
	<sql id="testColumns">
		a.id AS "id",
		a.name AS "name"
	</sql>
	
	<sql id="testJoins">
		
	</sql>	
	<insert id="insert">
		INSERT INTO test(
			id,
			name
		) VALUES (
			#{id},
			#{name}
		)
	</insert>
</mapper>